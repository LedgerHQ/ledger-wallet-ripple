      1000 / 20 : retriesCount < 40 + 60 ?
      // Then, for 1 minute: once per second
      1000 : retriesCount < 40 + 60 + 60 ?
      // Then, for 10 minutes: once every 10 seconds
      10 * 1000 :
      // Then: once every 30 seconds
      30 * 1000;
    }
