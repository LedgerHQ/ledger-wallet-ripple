    return this.toHex();} };



function ensureArrayLikeIs(Type, arrayLike) {
  return { 
    withChildren: function withChildren(Child) {
      if (arrayLike instanceof Type) {
        return arrayLike;}

      var obj = new Type();
      for (var i = 0; i < arrayLike.length; i++) {
        obj.push(Child.from(arrayLike[i]));}

      return obj;} };}




