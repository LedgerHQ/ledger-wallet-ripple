    this.slotBits = this.slotBits | 1 << slot;
    this.branches[slot] = branch;}, 

  empty: function empty() {
    return this.slotBits === 0;}, 

  hash: function hash() {
    if (this.empty()) {
      return Hash256.ZERO_256;}

