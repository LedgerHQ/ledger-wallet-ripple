      newInner.addItem(existing.index, null, existing);
      newInner.addItem(index, item, leaf);
      this.setBranch(nibble, newInner);} else 
    if (existing.isInner()) {
      existing.addItem(index, item, leaf);} else 
    {
      assert(false);}} });




