        },
        "threads": {
          "type": "number",
          "description": "*(Admin only)* The number of threads in the servers main job pool, performing various Ripple Network operations."
        }
      },
      "required": ["jobTypes", "threads"]
    },
    "lastClose": {
      "type": "object",
      "description": "Information about the last time the server closed a ledger.",
