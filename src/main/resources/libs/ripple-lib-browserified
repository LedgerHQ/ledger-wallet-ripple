  });
  ajv.addFormat('address', isValidAddress);
  ajv.addFormat('secret', isValidSecret);
  return ajv;
}

var ajv = loadSchemas();

function schemaValidate(schemaName, object) {
  var isValid = ajv.validate(schemaName, object);
  if (!isValid) {
    throw new ValidationError(ajv.errorsText());
  }
}

